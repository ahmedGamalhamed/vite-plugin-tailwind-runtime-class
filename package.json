{
 "name": "vite-plugin-tailwind-runtime-class",
 "version": "1.0.0",
 "description": "A Vite plugin that generates runtime Tailwind CSS classes by scanning files for class definitions",
 "main": "dist/index.js",
 "module": "dist/index.mjs",
 "types": "dist/index.d.ts",
 "exports": {
  ".": {
   "import": "./dist/index.mjs",
   "require": "./dist/index.js",
   "types": "./dist/index.d.ts"
  }
 },
 "files": [
  "dist",
  "README.md",
  "LICENSE"
 ],
 "scripts": {
  "build": "tsup",
  "dev": "tsup --watch",
  "prepublishOnly": "npm run build",
  "test": "echo \"Error: no test specified\" && exit 1"
 },
 "keywords": [
  "vite",
  "plugin",
  "tailwind",
  "tailwindcss",
  "runtime",
  "dynamic-classes",
  "css"
 ],
 "author": "Ahmed Hamed <ahmed.gamalhamed5@gmail.com>",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "https://github.com/ahmedGamalhamed/vite-plugin-tailwind-runtime-class.git"
 },
 "bugs": {
  "url": "https://github.com/ahmedGamalhamed/vite-plugin-tailwind-runtime-class/issues"
 },
 "homepage": "https://github.com/ahmedGamalhamed/vite-plugin-tailwind-runtime-class#readme",
 "peerDependencies": {
  "vite": ">=4.0.0"
 },
 "dependencies": {
  "fast-glob": "^3.3.2"
 },
 "devDependencies": {
  "@types/node": "^20.0.0",
  "tsup": "^8.5.0",
  "typescript": "^5.0.0",
  "vite": "^5.0.0"
 },
 "engines": {
  "node": ">=16.0.0"
 }
}
